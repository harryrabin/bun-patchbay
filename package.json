{
  "name": "bun-patchbay",
  "version": "0.1.0",
  "description": "A modular, declarative web framework for Bun",
  "main": "index.ts",
  "scripts": {
    "build": "rm -r ./dist 2>/dev/null; webpack && bun bun launch.ts",
    "dev": "export PB_ENV=dev && bun launch.ts",
    "launch": "export PB_ENV=prod && bun launch.ts",
    "clean": "rm -r ./dist ./node_modules 2>/dev/null",
    "reinstall": "rm -r ./node_modules 2>/dev/null; bun i"
  },
  "author": "Harrison Rabin <harry@rabinmgmt.com>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.6",
    "async-mutex": "^0.3.2",
    "bun-types": "^0.1.4",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21"
  }
}
